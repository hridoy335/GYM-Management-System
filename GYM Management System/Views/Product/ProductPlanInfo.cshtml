@model IEnumerable<GYM_Management_System.Models.ProductPlan>

@{
    ViewBag.Title = "ProductPlanInfo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ProductPlanInfo</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProductName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ProductName)
        </td>
        <td>
            @Html.ActionLink("Update", "ProductPlanUpdate", new { id=item.ProductPlanId }) |

        </td>
    </tr>
}

</table>
